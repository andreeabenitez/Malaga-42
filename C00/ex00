#include <unistd.h>

void	ft_putchar(char c)
{
	write(1, &c, 1);
}

int main()
{
	ft_putchar('H');
	ft_putchar('o');
	ft_putchar('l');
	ft_putchar('a');
	ft_putchar('\n');  // Salto de lÃ­nea
	return 0;
}
