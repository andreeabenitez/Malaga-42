#include <unistd.h>
#include <stdio.h>

void	ft_ft(int *nbr)
{
	*nbr = 42;
}

int     main()
{
        int n = 10;
        printf("before: %d\n", n);
        ft_ft(&n);
        printf("after: %d\n", n);
        return(0);
}
