#include <unistd.h>
#include <stdio.h>

void	ft_ultimate_ft(int *********nbr)
{
	*********nbr = 42;
}

int     main()
{
        int n = 10;
        int *ptr1 = &n;
        int **ptr2 = &ptr1;
        int ***ptr3 = &ptr2;
        int ****ptr4 = &ptr3;
        int *****ptr5 = &ptr4;
        int ******ptr6 = &ptr5;
        int *******ptr7 = &ptr6;
        int ********ptr8 = &ptr7;
        printf("Before: %d\n", n);
        ft_ultimate_ft(&ptr8);
        printf("After: %d\n", n);
        return 0;
}
